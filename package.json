{
  "name": "apotek-alpro-dashboard",
  "version": "1.0.0",
  "description": "Apotek Alpro Executive Dashboard - Mobile App",
  "type": "module",
  "main": "supabase-config.js",
  "scripts": {
    "build": "cp index.html mobile-styles.css mobile-styles-enhanced.css supabase-config.js www/ && cp -r assets www/ 2>/dev/null || true",
    "sync": "npm run build && npx cap sync",
    "open:android": "npx cap open android",
    "build:apk": "cd android && ./gradlew assembleDebug && cd ..",
    "build:apk-release": "cd android && ./gradlew assembleRelease && cd ..",
    "build:hybrid": "./build-hybrid.sh",
    "build:bundled": "./build-bundled.sh"
  },
  "keywords": ["apotek", "alpro", "dashboard", "mobile"],
  "author": "Apotek Alpro",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "node-fetch": "^3.3.2",
    "puppeteer": "^21.7.0"
  },
  "devDependencies": {
    "@capacitor/android": "^7.4.4",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4"
  }
}
